# Production Environment Variables for Vercel Deployment
# Copy these to your Vercel dashboard environment variables

# API Configuration
VITE_API_URL=https://your-backend-api.vercel.app/api
VITE_APP_NAME=MarketHub
VITE_APP_VERSION=1.0.0

# Environment
NODE_ENV=production
VITE_NODE_ENV=production

# Features
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_LOGGING=false

# Security (Set these in Vercel dashboard)
VITE_JWT_SECRET=5168942a2d3dcd03b1821276c59585bd8966940ab55829ed0e8d6c2990b6982338ebf9ebcc57fac173e07f7773d6a720268a33da81ae50d8b762de02318912bf
VITE_ENCRYPTION_KEY=your-encryption-key-here

# Database (MongoDB Atlas recommended for production)
VITE_MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/markethub?retryWrites=true&w=majority

# External Services (if using)
VITE_STRIPE_PUBLIC_KEY=pk_live_your_stripe_public_key
VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_HOTJAR_ID=your_hotjar_id

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
